<%- include("./partials/header") -%>
<%- include("./partials/nav") -%>

    <div class="container">
        <main>
            <% if (locals.user) { %>
                <h1>Welcome to your dashboard, <%= locals.firstName %> </h1>
    
                <% posts.forEach((post) => { %>
                    <% console.log(post, " :post") %>
                    <ul>
                        <li><h1><%= post.title %> </h1></li>
                        <li><p><%= post.post_data %></p></li>
                    </ul>
                <% }) %>
            <% } else { %>
                <h1>Welcome POTATO</h1>
            <% } %>
        </main>
    </div>
    


    <%- include("./partials/footer") -%>
